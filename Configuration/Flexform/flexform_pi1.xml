<T3DataStructure>
	<meta>
	<langDisable>1</langDisable>
	<!--  <langChildren>1</langChildren>!-->
		</meta>
<sheets>
	<tqSlideshow_main>
	<ROOT>
		<TCEforms>
			<sheetTitle>LLL:EXT:tq_slideshow/Resources/Private/Language/locallang_db.xml:tx_TqSlideshow.sheet_general</sheetTitle>
		</TCEforms>
		<type>array</type>
		<el>

            <settings.slideshow_id>
                <TCEforms>
                    <label>LLL:EXT:tq_slideshow/Resources/Private/Language/locallang_tca.xml:slideshow.collection_id</label>
                    <config>
                        <type>select</type>
                        <itemsProcFunc>tx_tqslideshow_slideshowlist->user_collection</itemsProcFunc>
                    </config>
                </TCEforms>
            </settings.slideshow_id>

            <settings.videoWidth>
                <TCEforms>
                    <label>LLL:EXT:tq_slideshow/Resources/Private/Language/locallang_tca.xml:slideshow.videoWidth</label>
                    <config>
                        <type>input</type>
                        <size>5</size>
                        <max>5</max>
                        <eval>int</eval>
                        <checkbox>0</checkbox>
                        <default></default>
                    </config>
                </TCEforms>
            </settings.videoWidth>

            <settings.videoHeight>
                <TCEforms>
                    <label>LLL:EXT:tq_slideshow/Resources/Private/Language/locallang_tca.xml:slideshow.videoHeight</label>
                    <config>
                        <type>input</type>
                        <size>5</size>
                        <max>5</max>
                        <eval>int</eval>
                        <checkbox>0</checkbox>
                        <default></default>
                    </config>
                </TCEforms>
            </settings.videoHeight>


            <settings.showToolbar>
                <TCEforms>
                    <label>LLL:EXT:tq_slideshow/Resources/Private/Language/locallang_tca.xml:slideshow.showToolbar</label>
                    <onChange>reload</onChange>
                    <config>
                        <type>check</type>
                        <default>1</default>
                    </config>
                </TCEforms>
            </settings.showToolbar>

            <settings.showThumbnails>
                <TCEforms>
                    <label>LLL:EXT:tq_slideshow/Resources/Private/Language/locallang_db.xml:tx_TqSlideshow.showThumbnails</label>
                    <onChange>reload</onChange>
                    <config>
                        <type>check</type>
                        <default>1</default>
                    </config>
                </TCEforms>
            </settings.showThumbnails>

            <settings.showPaging>
                <TCEforms>
                    <label>LLL:EXT:tq_slideshow/Resources/Private/Language/locallang_tca.xml:slideshow.showPaging</label>
                    <onChange>reload</onChange>
                    <config>
                        <type>check</type>
                        <default>1</default>
                    </config>
                </TCEforms>
            </settings.showPaging>
			</el>
		</ROOT>
	</tqSlideshow_main>

    <tqSlideshow_template>
        <ROOT>
            <TCEforms>
                <sheetTitle>LLL:EXT:tq_slideshow/Resources/Private/Language/locallang_db.xml:tx_TqSlideshow.sheet_template</sheetTitle>
            </TCEforms>
            <type>array</type>
            <el>
                <view.templateRootPath>
                    <TCEforms>
                        <label>LLL:EXT:tq_slideshow/Resources/Private/Language/locallang_db.xml:tx_TqSlideshow.templateRootPath</label>
                        <config>
                            <type>input</type>
                            <size>30</size>
                            <max>30</max>
                            <eval>trim</eval>
                        </config>
                    </TCEforms>
                </view.templateRootPath>

                <view.partialRootPath>
                    <TCEforms>
                        <label>LLL:EXT:tq_slideshow/Resources/Private/Language/locallang_db.xml:tx_TqSlideshow.partialRootPath</label>
                        <config>
                            <type>input</type>
                            <size>30</size>
                            <max>30</max>
                            <eval>trim</eval>
                        </config>
                    </TCEforms>
                </view.partialRootPath>

                <view.layoutRootPath>
                    <TCEforms>
                        <label>LLL:EXT:tq_slideshow/Resources/Private/Language/locallang_db.xml:tx_TqSlideshow.layoutRootPath</label>
                        <config>
                            <type>input</type>
                            <size>30</size>
                            <max>30</max>
                            <eval>trim</eval>
                        </config>
                    </TCEforms>
                </view.layoutRootPath>
            </el>
        </ROOT>
    </tqSlideshow_template>




</sheets>
</T3DataStructure>