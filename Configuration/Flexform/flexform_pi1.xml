	<tqSlideshow_settings>
	<ROOT>
		<TCEforms>
			<sheetTitle>LLL:EXT:tq_slideshow/Resources/Private/Language/locallang_db.xml:tx_TqSlideshow.sheet_settings</sheetTitle>
		</TCEforms>
		<type>array</type>
		<el>
			<settings.templateFile>
				<TCEforms>
					<label>LLL:EXT:tq_slideshow/Resources/Private/Language/locallang_db.xml:tx_TqSlideshow.templateFile</label>
					<config>
						<type>group</type>
						<internal_type>file_reference</internal_type>
						<uploadfolder>uploads/tx_tqkueffnerproduct2</uploadfolder>
						<allowed>html,htm,tmpl</allowed>
						<max_size>1000</max_size>
						<size>1</size>
						<maxitems>1</maxitems>
						<minitems>0</minitems>
					</config>
				</TCEforms>
			</settings.templateFile>

			<settings.containerWidth>
				<TCEforms>
					<label>LLL:EXT:tq_slideshow/Resources/Private/Language/locallang_db.xml:tx_TqSlideshow.containerWidth</label>
					<config>
						<type>input</type>
						<size>5</size>
						<max>5</max>
						<eval>trim</eval>
						<checkbox>0</checkbox>
						<default></default>
					</config>
				</TCEforms>
			</settings.containerWidth>

			<settings.containerHeight>
				<TCEforms>
					<label>LLL:EXT:tq_slideshow/Resources/Private/Language/locallang_db.xml:tx_TqSlideshow.containerHeight</label>
					<config>
						<type>input</type>
						<size>5</size>
						<max>5</max>
						<eval>trim</eval>
						<checkbox>0</checkbox>
						<default></default>
					</config>
				</TCEforms>
			</settings.containerHeight>

			<settings.keyEvents>
				<TCEforms>
					<label>LLL:EXT:tq_slideshow/Resources/Private/Language/locallang_db.xml:tx_TqSlideshow.keyEvents</label>
					<config>
						<type>check</type>
						<default>1</default>
					</config>
				</TCEforms>
			</settings.keyEvents>
	<!--
			<settings.showDescription>
				<TCEforms>
					<label>LLL:EXT:tq_slideshow/Resources/Private/Language/locallang_db.xml:tx_TqSlideshow.showDescription</label>
					<config>
						<type>check</type>
						<default>1</default>
					</config>
				</TCEforms>
			</settings.showDescription>

	-->
		</el>
	</ROOT>
	</tqSlideshow_settings>

</sheets>
</T3DataStructure>